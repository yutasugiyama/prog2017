ñ‚157
def total(n):
  s = 0
  for i in range(1,n + 1 ):
    s += i
   return s

total(5)

total(10)

ñ‚158
import turtle
t = turtle.Turtle()

def center_circle():
  t.penup()
  t.forward(200)
  t.left(90)
  t.pendown()
  t.circle(200)
  t.left(90)
  t.penup()
  t.forward(200)
  t.pendown()

center_circle()

