import turtle
t = turtle.Turtle()

def center_circle(x, y, r):
  t.up()
  t.home()
  t.forward(x)
  t.left(90)
  t.forward(y)
  t.forward(z)
  t.left(90)
  t.down()
  t.circle(r)
 
t.home()
t.clear()

circle(0, 0, 100)
circle(100, 200, 50)

ñ‚162
def circle(x, y, r = 150):
  t.up()
  t.home()
  t.forward(x)
  t.left(90)
  t.forward(y)
  t.forward(r)
  t.left(90)
  t.down()
  t.circle(r)

t.home()
t.clear()

circle(100, -100)
circle(100, 100, 50)
circle(-100, -100, 50)
circle(-100, 100, 50)